{
  "manifest_version": 2,

  "name": "sample_chromeExtension",
  "description": "extractor",
  "version": "2.0.0",

 "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
     "background",
    "tabs",
    "<all_urls>",
	  "storage"
  ]
}